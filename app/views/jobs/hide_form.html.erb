<%= simple_form_for(@job) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :job_type %>
    <%= f.input :occupation %>
    <%= f.input :company_name %>
    <%= f.input :location %>
    <%= f.input :url %>
    <%= f.input :description %>
    <%= f.input :apply_information %>
    <%= f.input :deadline %>
    <%= f.input :user_id %>
    <%= f.input :aasm_state %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
